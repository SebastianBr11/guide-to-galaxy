{
	"root": true,
	"env": { "node": true },
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "jest"],
	"extends": ["eslint:recommended", "plugin:@typescript-eslint/recommended"],
	"rules": {
		"@typescript-eslint/no-unused-vars": [
			"error",
			{
				"varsIgnorePattern": "^_",
				"argsIgnorePattern": "^_"
			}
		]
	}
}
